{
    "name": "netcrave/pscr_base",
    "type": "project",
    "require": {
        "netcrave/pscr_core": "dev-master"
    },
    "repositories": [
    {
        "type": "package",
        "package": {
            "name": "netcrave/pscr_core",
            "version": "dev-master",
            "source": {
                "url": "https://github.com/netcrave/pscr_core.git",
                "type": "git",
                "reference": "master"
            },
            "autoload": {
                "psr-4": {
                    "pscr\\lib\\": "lib/"
                }
            }
        }
    }],
    "autoload": {
        "psr-0": {
            "": "apps/"
        }
    }
}
